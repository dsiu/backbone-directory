<div class="page-header">
    <h1>New employee</h1>
</div>

<% form_for(employee, {action: path_to.employees(), method: 'POST', id: "employee_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('employees/form.ejs', {locals: {form: form, employee: employee}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create employee', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.employees(), {class: 'btn'}) %>
    </div>
<% });%>
